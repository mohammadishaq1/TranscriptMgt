        <script type="text/javascript" src="~/Content/AdminTemplate/js/jquery.nav.js"></script>
        <script type="text/javascript" src="~/Content/AdminTemplate/syntax-highlighter/scripts/shCore.js"></script>
        <script type="text/javascript" src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushXml.js"></script>
        <script type="text/javascript" src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushCss.js"></script>
        <script type="text/javascript" src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushJScript.js"></script>
        <script type="text/javascript" src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushPhp.js"></script>*@
    <script src="~/Content/AdminTemplate/js/jquery-1.11.0.min.js"></script>
    <script src="~/Content/AdminTemplate/js/jquery.nav.js"></script>
    <script src="~/Content/AdminTemplate/syntax-highlighter/scripts/shCore.js"></script>
    <script src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushXml.js"></script>
    <script src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushCss.js"></script>
    <script src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushJScript.js"></script>
    <script src="~/Content/AdminTemplate/syntax-highlighter/scripts/shBrushPhp.js"></script>
    <script type="text/javascript">
        SyntaxHighlighter.all()
    </script>
    <script type="text/javascript" src="~/Content/AdminTemplate/js/custom.js"></script>


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="~/Content/StartupTemplate/js/jquery-2.2.4.min.js"></script>
    <script src="~/Content/StartupTemplate/js/popper.js"></script>
    <script src="~/Content/StartupTemplate/js/bootstrap.min.js"></script>
    <script src="~/Content/StartupTemplate/js/stellar.js"></script>
    <script src="~/Content/StartupTemplate/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Content/StartupTemplate/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/Content/StartupTemplate/js/waypoints.min.js"></script>
    <script src="~/Content/StartupTemplate/js/mail-script.js"></script>
    <script src="~/Content/StartupTemplate/js/contact.js"></script>
    <script src="~/Content/StartupTemplate/js/jquery.form.js"></script>
    <script src="~/Content/StartupTemplate/js/jquery.validate.min.js"></script>
    <script src="~/Content/StartupTemplate/js/mail-script.js"></script>
    <script src="~/Content/StartupTemplate/js/theme.js"></script>


</body>
</html>
